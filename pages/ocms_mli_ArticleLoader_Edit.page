                <tr>
                    <td>
                        <label class="ocmsLabel" for="sTaxonomyName" title="">Taxonomy Name <a class="ocmsHint"
                            onclick="$(this).parentsUntil('td').siblings('.ocmsInstructions').slideToggle()">[?]</a></label>


                        <div class='ocmsInstructions' style='display: none'>
                            <p>
                                An exact taxonomy name who's children nodes will be used to generate the 'In-Page
                                Filter' controls, ex.
                                <kbd>Brand Taxonomy</kbd>
                                . Following assumptions are in effect:
                            </p>



                            <ul>
                                <li>First children of taxonomy are the filter headings.</li>
                                <li>Second level children are the selectable filters.</li>
                                <li>Only first and second level children are rendered.</li>
                                <li>Any taxonomy nodes (and its children) prefixed with the <kbd>*</kbd> are
                                    hidden.
                                </li>
                            </ul>
                        </div>


                        <input required="required" type="text" id="sTaxonomyName" name="sTaxonomyName" value="{!sTaxonomyName}"></input>
                        <apex:outputText rendered="{!disableAll == false}">
                        <script>
                        var $oTaxonomyNameElement =  $(document.currentScript).prev(); // need to store the global reference for our callback
                        var $oReplacementSelectElement = $('<select id="' + $oTaxonomyNameElement.attr('id') + '" required="' + $oTaxonomyNameElement.attr('required') + '" name="' + $oTaxonomyNameElement.attr('name') + '"></select>'); // http://stackoverflow.com/questions/7747266/why-do-people-use-slashes-when-creating-elements
                        $.ajax({
                            async: false, /* because we don't like re-draws and taxonomy node selectors later on depends on new markup */
                            url: '/apex/ServiceEndpoint?ocmsLang=' + $(document).data('cms').languageInUse.LanguageCode,
                            data: {
                                    action: 'getAllTaxonomies',
                                    service: 'OrchestraPublicAjax',
                                    runtime: 'Application',
                                    sname:   $(document).data('cms')['site_name'],
                                    token:   $(document).data('cms')['csrf_token']
                                },
                            type: 'POST',
                            dataType: 'json',
                            success: function _getAllTaxonomies_SuccessHandler (json, textStatus, xhr) {

                                var taxonomyList = json;

                                // debug.debug('getAllTaxonomies says:', json, textStatus);

                                var html = '';
                                if (taxonomyList != undefined) {
                                    $.each(taxonomyList, function() {
                                        var txName = this.name,
                                            disabled = '';
                                        if (!txName || txName === 'null') {
                                            txName = 'untranslated taxonomy [' + this.id + ']';
                                            disabled = ' disabled ';
                                        }
                                        var selected = (txName === $oTaxonomyNameElement.val()) ? ' selected ' : '';
                                        html += '<option data-id="' + this.id + '" data-name="' + txName + '"' + disabled + selected + '>' + txName + '</option>';
                                    });
                                }
                                $oReplacementSelectElement.html(html);
                                $oTaxonomyNameElement.replaceWith($oReplacementSelectElement);

                            }
                        });

                        //# sourceURL=ocms_mli_ArticleLoader_Edit#inlinedScript2
                        </script>
                        </apex:outputText>

