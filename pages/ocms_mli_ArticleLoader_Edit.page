                        <input required="required" type="text" id="sTaxonomyTimeIntervalNodeDefaultFilter" name="sTaxonomyTimeIntervalNodeDefaultFilter"
                            value="{!sTaxonomyTimeIntervalNodeDefaultFilter}"></input>

                        <apex:outputText rendered="{!disableAll == false}">
                        <script>
                        function fClosure() {
                            var $sTaxonomyPathInput =  $(document.currentScript).prev(); // need to store the global reference for our callback
                            $sTaxonomyPathInput.attr( "readonly", true);

                            var $changeTaxonomyButton = $('<button class="changeMainTaxonomy ui-button ui-state-default ui-corner-all ui-button-text-only"><span class="ui-button-text">Select Path</span></button>');
                            $sTaxonomyPathInput.after($changeTaxonomyButton);

                            $changeTaxonomyButton.click(
                                    function fChangeMainTaxonomyClicked() {
                                        // Invoke the ocmsTaxonomyBrowser widget
                                        $("<div></div>").ocmsTaxonomyBrowser({
                                            // when this runs it's
                                            taxonomyId: $("#sTaxonomyName option:selected").attr('data-id')
