            href="{!URLFOR($Resource.ocms_mli_pageSupport, 'css/ocms-reset+support-v2-edit-content.css')}" />
        <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.ocms_mli_pageSupport, 'css/ps-mli-edit-content.css')}" />
        <!-- <script src="{!URLFOR($Resource.ocms_mli_pageSupport, 'js/tagPathSelector.js')}"></script> -->
        <style>
        *[required] {
        font-weight: bold; /* IMPRV: make this a better style or consistent with OCMS */
        }
        </style>

        <script>
            /*
            To overide OCMS default attribute properites or to store non simple values, i.e. those that are not
            `"type": "Text", "simple": true` you can do so by attaching `data-*` attributes to the input
            elements, example for a language neutral attriubte:


            *NOTE*: SF is stripping empty attributes!! `data-lang=""` so we must pass someething, in our case we pass " "
                    and have an exception edge case handler
                    TODO: post an idea to SF to stop doing this.
            ```
            <input data-lang=" " data-type='FOOBAR' data-simple=false class="ocmsLanguageNeutral" type="number"
                min="1" id="iQueryLimit" name="iQueryLimit" value="{!iQueryLimit}"></input>
            ```
            */

            // we must attach editor save after dom ready as `ce` will not be available before that
            $(document).ready(function fAttachRegisterSaveFunction() {
                ce.content_editor('registerSaveFunction', function() {
                    var aFormElement = $('#idContentEditor');
                    var aFormArray =  aFormElement.serializeArray();
                    for(var i = 0; i < aFormArray.length; i++) {
                        var oSerializedInput = aFormArray[i];
                        // let's extract data-* attributes as well so that we can pass things like `"type": "Text", "simple": true` through the DOM
                        // 2015-Aug-14,DS - FYI those seem to be optional and what OCMS defaults in the back end
                        var oInputElement = $("[name='" + oSerializedInput.name + "']", aFormElement)
                        if (oInputElement.data('lang') !== undefined) {
                            oInputElement.data('lang',oInputElement.data('lang').trim()); // handling data-lang=" "
                        }
                        aFormArray[i] = $.extend(oSerializedInput, oInputElement.data());
                    }
                    return aFormArray;
                });
            });

            // Debuging script reference for Chrome Developer Tools
            //# sourceURL=ocms_mli_ArticleLoader_Edit#inlinedScript1
        </script>

        <!-- wrapping this in form allows us to use jQuery's form.serializeArray -->
        <form id='idContentEditor' class='ocmsContentEditor' action="javascript:false">
