                                                $sTaxonomyPathInput.val(tagPath);
                                            }
                                        });

                                        return false; /* this prevents the default button submit action */
                            });
                        }
                        fClosure();

                        //# sourceURL=ocms_mli_ArticleLoader_Edit#inlinedScript4
                        </script>
                        </apex:outputText>

                    </td>
                </tr>

                <!--  the below stores taxonomy node tag def id, and the tag path (we do that as there at least 7.89.12 no easy way to get the path if we just have tag path id, we could try using `getChildren` in TaxonomyManagerAjaxController class -->
              <tr>
                    <td>
                        <label class="ocmsLabel" for="sTaxonomyMandatoryFilterNode" title="">Taxonomy
                            Mandatory Filter Node <a class="ocmsHint"
                            onclick="$(this).parentsUntil('td').siblings('.ocmsInstructions').slideToggle()">[?]</a>
                        </label>


                        <div class='ocmsInstructions' style='display: none'>
                            <p>
                                If specified, this taxonomy node will be ANDed with any other filters specified effectively
                                restricting the return set to this node and it's children.
                            </p>

                            <p>
                                The node will be rendered as 'checked', 'disabled', and 'readonly' controls and will
                                not be leveraged in the filtering logic of the form rendered on the page.
                            </p>

                            <p>Example: <kbd>Brand Taxonomy/Location/Global</kbd></p>
                        </div>
                        <input type="text" id="sTaxonomyMandatoryFilterNode" name="sTaxonomyMandatoryFilterNode"
                            value="{!sTaxonomyMandatoryFilterNode}"></input>

                        <script>
                        // Script to get taxonomy node `tagDefId` instead of path, which is the unqiue id of parent of same translated tax nodes

                        function fClosure() { // the closure prevents our variables from being part of global scope and allows same code to be reused on other inputs i.e. $sTaxonomyPathInput won't get overwritten by subsequent copy of this script
                            var bReadyOnly = {!disableAll}; // use to determine if controls should be disabled, ex. content is pubished.
                            // we want to present a human readable path to the end user instead of the taxDefId that we store so we hide the raw input and present a human readable box

                            var $sTaxonomyPathInput =  $(document.currentScript).prev(); // need to store the global reference for our callback
                            $sTaxonomyPathInput.attr( "readonly", true);
                            $sTaxonomyPathInput.hide();
                            var $TaxonomyNodePathElement = $('<div/>').html('<input type="text" disabled="true" readonlly="true">').contents(); // parseHTML exists starting jQuery 1.8, `disabled` means it does not get sent to server or returned by `$(<form>).serializeArray()`
                            try { // any parsing issues "Uncaught SyntaxError: Unexpected end of input(…)" just catch
                                if ( typeof (JSON.parse($sTaxonomyPathInput.val())).path !== "undefined") {
                                    $TaxonomyNodePathElement.val(JSON.parse($sTaxonomyPathInput.val()).path);
                                }
                            } catch (e) {}
                            $sTaxonomyPathInput.after($TaxonomyNodePathElement); // add our new input to the dom

                            // if we are disabled then don't allow editing but still display the mapped taxonoomy rather then raw data
                            if (bReadyOnly == false) {
                                var $changeTaxonomyButton = $('<button class="changeMainTaxonomy ui-button ui-state-default ui-corner-all ui-button-text-only"><span class="ui-button-text">Select Path</span></button>');
                                $TaxonomyNodePathElement.after($changeTaxonomyButton);

                                $changeTaxonomyButton.click(
                                        function fChangeMainTaxonomyClicked() {
                                            // Invoke the ocmsTaxonomyBrowser widget
                                            $("<div></div>").ocmsTaxonomyBrowser({
                                                // when this runs it's
                                                taxonomyId: $("#sTaxonomyName option:selected").attr('data-id')
                                                ,taxonomyName:  $("#sTaxonomyName option:selected").val()
                                                ,token: $(document).data('cms')['csrf_token']
                                                ,languageId: $(document).data('cms').languageInUse.Id
                                                ,disabled: false
                                                ,singleSelect:true
                                                ,returnTagDefs: true
                                                ,onGetSelectedTags: function (oTags) {
                                                    // path selected
                                                    if (oTags[0] != undefined && 'path' in oTags[0]) {
                                                       $sTaxonomyPathInput.val(JSON.stringify(oTags[0]));
                                                       $TaxonomyNodePathElement.val(oTags[0].path);
                                                    }
                                                    // no path selected
                                                    else {
                                                       $sTaxonomyPathInput.val("");
                                                       $TaxonomyNodePathElement.val("");
                                                    }
                                                }
                                            });

                                            return false; /* this prevents the default button submit action */
                                });
                            }
                        }
                        fClosure();

                        //# sourceURL=ocms_mli_ArticleLoader_Edit#inlinedScript5
                        </script>

                    </td>
                </tr>



