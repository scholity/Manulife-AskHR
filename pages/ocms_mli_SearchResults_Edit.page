<!--
OrchestraCMS Edit Page
######################-->
<apex:page controller="cms.CreateContentController" extensions="ocms_mli_Search" showHeader="false" sidebar="false"
    cache="false" title="Orchestra CMS" standardStylesheets="false">

    <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.ocms_mli_pageSupport, 'css/ocms-reset+support-v2-edit-content.css')}" />
    <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.ocms_mli_pageSupport, 'css/ps-mli-edit-content.css')}" />
    <style>
/* ... */
</style>

    <script>
        // we must attach editor save after dom ready as `ce` will not be available before that
        $(document).ready(function initContentEditor() {
            ce.content_editor('registerSaveFunction', function() {
                return $('#idContentLayoutEditor').serializeArray();
            });
        });

        /*
        If you are storing non simple values `serializeArray()` might not work and you would have to use below approach
        ```
        ce.content_editor('registerSaveFunction', function(){
            var attributes = new Array();

            attributes.push({"name":"sQueryTermInputPlaceholder", "value": $('#sQueryTermInputPlaceholder').val(), "type": "Text", "simple": true});
            attributes.push({"name":"sIconSearchLabel", "value": $('#sIconSearchLabel').val(), "type": "Text", "simple": true});
            attributes.push({"name":"sQueryTermInputPlaceholder", "value": $('#sQueryTermInputPlaceholder').val(), "type": "Text", "simple": true});
            attributes.push({"name":"sButtonSubmitLabel", "value": $('#sButtonSubmitLabel').val(), "type": "Text", "simple": true});

            return attributes;
        });
        ```
         */

        // Debuging script reference for Chrome Developer Tools
        //# sourceURL=ocms_mli_SearchResults_Edit#inlinedScript1
    </script>

    <apex:outputPanel >
        <!--  even though we never do a POST wrapping our inputs in a `form` allows us to use jQuery's `.serializeArray` -->
        <!-- see `ocms.contenteditor.css` for available classes -->
        <form id='idContentLayoutEditor'>
            <table class="ocmsContentEditor ocmsContentEditorNested">
                <!-- superseded by OMCS7.x adding a language heading with it's own `sContentLayoutLabel`
                <tr>
                    <td colspan="2">
                        <h2>{!content.cms__Name__c}</h2>
                        <h3>{!sContentLayoutLabel}</h3> <apex:outputText rendered="{!disableAll}">
                            <p class="ui-state-info">This is published content. To change its value, create a new revision or version.</p>
                        </apex:outputText>
                    </td>
                    </tr> -->

                <tr>
                    <td>
                    <div class="ocmsHint">
                    <p>
                    {!sContentLayoutDescription}
                    <a onclick="$(this).parentsUntil('td').siblings('.ocmsInstructions').slideToggle()">[?]</a>
                    </p>
                    <div style="display: znone" class="ocmsInstructions">
                    <img style="width: 90%" src="data:image/png;base64," />
                    </div>
                    </div>
                    </td>
                </tr>


                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sQueryTermInputPlaceholder">1. Query
                            Term Input Placeholder</label> <input required="required" id="sQueryTermInputPlaceholder"
                        class="ocmsTextInputLrg" name="sQueryTermInputPlaceholder" type="text"
                        value="{!sQueryTermInputPlaceholder}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sSortByLabel">2. Sort by Label</label> <input
                        required="required" id="sSortByLabel" class="ocmsTextInputLrg" name="sSortByLabel" type="text"
                        value="{!sSortByLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sSortByAscendingLabel">3. Sort By
                            Relevance Label</label> <input required="required" id="sSortByAscendingLabel"
                        class="ocmsTextInputLrg" name="sSortByAscendingLabel" type="text"
                        value="{!sSortByAscendingLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sSortByDescendingLabel">3. Sort By
                            Date Descending Label</label> <input required="required" id="sSortByDescendingLabel"
                        class="ocmsTextInputLrg" name="sSortByDescendingLabel" type="text"
                        value="{!sSortByDescendingLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sTabWorldwideLabel">4. Tab Worldwide
                            Label</label> <input required="required" id="sTabWorldwideLabel" class="ocmsTextInputLrg"
                        name="sTabWorldwideLabel" type="text" value="{!sTabWorldwideLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sTabLocalLabel">5. Tab Local Label</label> <input
                        required="required" id="sTabLocalLabel" class="ocmsTextInputLrg" name="sTabLocalLabel"
                        type="text" value="{!sTabLocalLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sResultInformationHTML">6. Result
                            Information (HTML allowed) <a class="ocmsHint"
                            onclick="$(this).parentsUntil('td').siblings('.ocmsInstructions').slideToggle()">[?]</a>
                    </label>
                        <div class='ocmsInstructions' style='display: none'>
                              <p>
                                Specified value must contain special wrapping <em>placeholders</em> HTML tags as listed
                                below for the search summary to properly render:
                            </p>



                             <ul>
                                <li><strong>&lt;span
                                        class=&quot;resultTextEmphasis&quot;&gt;Result:&lt;/span&gt;</strong>: an optional style
                                    <kbd>span</kbd> that is used to emphasise text</li>
                                <li><strong>&lt;span class=&quot;resultFrom&quot;&gt;0&lt;/span&gt;</strong>:
                                    required <kbd>span</kbd> that is a placeholder for <em>result from</em> count number
                                </li>
                                <li><strong>&lt;span class=&quot;resultTo&quot;&gt;0&lt;/span&gt;</strong>:
                                    required <kbd>span</kbd> that is a placeholder for <em>result to</em> count number</li>
                                <li><strong>&lt;span class=&quot;resultCounter&quot;&gt;0&lt;/span&gt;</strong>:
                                    required <kbd>span</kbd> that is a placeholder for <em>page</em> counter</li>
                                <li><strong>&lt;span class=&quot;query&quot;&gt;&lt;/span&gt;</strong>: required <kbd>span</kbd>
                                    that is the placeholder for <em>total results</em> number</li>
                            </ul>

                               <h4>Example:</h4>
                                <p>
                                <kbd>&lt;span class=&quot;resultTextEmphasis&quot;&gt;Result:&lt;/span&gt;
                                    (&lt;span class=&quot;resultFrom&quot;&gt;0&lt;/span&gt;-&lt;span
                                    class=&quot;resultTo&quot;&gt;0&lt;/span&gt; OF &lt;span
                                    class=&quot;resultCounter&quot;&gt;0&lt;/span&gt;) of about '&lt;span
                                    class=&quot;query&quot;&gt;&lt;/span&gt;' from;</kbd>
                            </p>
                            <h4>Will render as:</h4>
                            <span class="resultTextEmphasis">Result:</span>
                            (<span class="resultFrom">0</span>-<span class="resultTo">0</span> OF <span class="resultCounter">0</span>) of about '<span class="query"></span>' from <em>&lt;selected-tab-label&gt;</em>

                        </div>

                         <input required="required" id="sResultInformationHTML" class="ocmsTextInputLrg"
                        name="sResultInformationHTML" type="text" value="{!sResultInformationHTML}"></input>


                        </td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel" for="sResultsLabel">7. Results Label</label> <input
                        id="sResultsLabel" class="ocmsTextInputLrg" name="sResultsLabel"
                        type="text" value="{!sResultsLabel}"></input></td>
                </tr>

                <tr>
                    <td><label class="ocmsLabel ocmsRequiredLabel" for="sNoResults">8. No Results Message</label> <input
                        required="required" id="sNoResults" class="ocmsTextInputLrg" name="sNoResults"
                        type="text" value="{!sNoResults}"></input></td>
                </tr>
            </table>
        </form>
    </apex:outputPanel>
</apex:page>