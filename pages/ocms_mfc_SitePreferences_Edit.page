    <apex:outputPanel layout="block">
        <script type="text/javascript" language="javascript">
            var $$ = $.noConflict(true),
                selectize,
                $select,
                apps;

            $(document).ready(function() {

            	function initCheckBoxes () {
            		$('#socialLinksInNewTab').attr('checked', '{!socialLinksInNewTab}' === 'true');
            	}

                function initSelectize () {
                    $select = $$('#mandatoryApps').selectize({
                        plugins: ['remove_button', 'drag_drop'],
                        delimiter: ',',
                        create: false
                    });

                    selectize = $select[0].selectize;
                }

                $.orchestracmsRestProxy.doAjaxServiceRequest('ocms_mfc_AppService', {
                        action: 'getFullAppListByName',
                    },
                    function (textStatus, json, xhr) {
                        var mandatoryAppIDs = '{!mandatoryAppIDs}'.split(',');
                        var mandatoryAppNames = '{!mandatoryAppNames}'.split(',');
                        var appFoundIndex = 0;

                        if (json.success) {
                            apps = json.apps;

                            for (var i = 0; i < apps.length; i++) {
                                var appName = apps[i].appName;

                                if (apps[i].duplicateName) {
                                    appName += ' - (' + apps[i].appCategory + ')';
                                }

                                selectize.addOption({value: apps[i].appId + ';' + appName, text: appName});

                                appFoundIndex = mandatoryAppIDs.indexOf(apps[i].appId);

                                if (appFoundIndex != -1) {
                                    selectize.addItem(mandatoryAppIDs[appFoundIndex] + ';' + mandatoryAppNames[appFoundIndex]);
                                }
                            }
                        }
                    }
                );

                initSelectize();

                ce.content_editor('registerSaveFunction', function () {
                    $.orchestracmsRestProxy.doAjaxServiceRequest('ocms_mfc_SitePreferences', {
                            action: 'setupSitePreferences',
                            thisContentId: "{!thisContentId}"
                        },
                        function (textStatus, json, xhr) {
                            debug.log('setupSitePreferences returned:', textStatus, json);
                        }
                    );

                    var apps = selectize.getValue().split(',');
                    var mandatoryAppIDs = '';
                    var mandatoryAppNames = '';

                    for (var i = 0; i < apps.length; i++) {
                        var app = apps[i].split(';')
                        mandatoryAppIDs += app[0] + ',';
                        mandatoryAppNames += app[1] + ',';
                    }

                    var attributes = [
                        {type: 'Text',  name: 'locationTaxonomy',                   value: $('#locationTaxonomy').val()},
                        {type: 'Text',  name: 'divisionTaxonomy',                   value: $('#divisionTaxonomy').val()},
                        {type: 'Text',  name: 'employeeTypeContributorTaxonomy',    value: $('#employeeTypeContributorTaxonomy').val()},
                        {type: 'Text',  name: 'employeeTypePeopleManagerTaxonomy',  value: $('#employeeTypePeopleManagerTaxonomy').val()},
                        {type: 'Text',  name: 'newsCategoryTaxonomy',               value: $('#newsCategoryTaxonomy').val()},
                        {type: 'Text',  name: 'appCategoriesTaxonomy',              value: $('#appCategoriesTaxonomy').val()},
                        {type: 'Text',  name: 'articleCategoriesTaxonomy',          value: $('#articleCategoriesTaxonomy').val()},
                        {type: 'Text',  name: 'featuredItemTagPath',                value: $('#featuredItemTagPath').val()},
                        {type: 'Text',  name: 'mandatoryNewsPaths',                 value: $('#mandatoryNewsPaths').val()},
                        {type: 'Text',  name: 'mandatoryAppIDs',                    value: mandatoryAppIDs.slice(0, -1)},
                        {type: 'Text',  name: 'mandatoryAppNames',                  value: mandatoryAppNames.slice(0, -1)},

                        {type: 'Text',  name: 'reportButtonEmail',                  value:$('#reportButtonEmail').val()},

