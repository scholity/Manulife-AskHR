                        {type: 'Text',  name: 'articlePage',                        value: $('#articlePage').val()},
                        {type: 'Text',  name: 'bannerPage',                         value: $('#bannerPage').val()},
                        {type: 'Text',  name: 'ceoArticlePage',                     value: $('#ceoArticlePage').val()},
                        {type: 'Text',  name: 'appLibrary',                         value: $('#appLibrary').val()},
                        {type: 'Text',  name: 'appDetail',                          value: $('#appDetail').val()},
                        {type: 'Text',  name: 'categoryDetail',                     value: $('#categoryDetail').val()}, // should have been appCategoryDetail
                        {type: 'Text',  name: 'eventPage',                          value: $('#eventPage').val()},
                        {type: 'Text',  name: 'calendarPage',                       value: $('#calendarPage').val()},
                        {type: 'Text',  name: 'eventFormPage',                      value: $('#eventFormPage').val()},
                        {type: 'Text',  name: 'ethicsHotlinePage',                  value: $('#ethicsHotlinePage').val()},
                        {type: 'Text',  name: 'userPrefsPage',                      value: $('#userPrefsPage').val()},
                        {type: 'Text',  name: 'mfConnectDirectory',                 value: $('#mfConnectDirectory').val()},

                        {type: 'Text', name: 'facebookPage',                        value: $('#facebookPage').val()},
                        {type: 'Text', name: 'twitterPage',                         value: $('#twitterPage').val()},
                        {type: 'Text', name: 'instagramPage',                       value: $('#instagramPage').val()},
