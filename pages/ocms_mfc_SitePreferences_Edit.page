                        {type: 'Text',  name: 'uiLabelBookmark',                    value: $('#uiLabelBookmark').val()},
                        {type: 'Text',  name: 'uiLabelShare',                       value: $('#uiLabelShare').val()},
                        {type: 'Text',  name: 'uiLabelArtContactInformation',       value: $('#uiLabelArtContactInformation').val()},
                        {type: 'Text',  name: 'uiLabelArtPrevious',                 value: $('#uiLabelArtPrevious').val()},
                        {type: 'Text',  name: 'uiLabelArtNext',                     value: $('#uiLabelArtNext').val()},
                        {type: 'Text',  name: 'uiLabelArtTopics',                   value: $('#uiLabelArtTopics').val()},
                        {type: 'Text',  name: 'uiLabelArtRelated',                  value: $('#uiLabelArtRelated').val()},
                        {type: 'Text',  name: 'uiLabelArtReadMore',                 value: $('#uiLabelArtReadMore').val()},

                        {type: 'Text',  name: 'uiNewsItemLinkText',                 value: $('#uiNewsItemLinkText').val()},
                        {type: 'Text',  name: 'uiSocialPostLinkText',               value: $('#uiSocialPostLinkText').val()},
                        {type: 'Text',  name: 'uiLabelLike',                        value: $('#uiLabelLike').val()},
                        {type: 'Text',  name: 'uiLabelToday',                       value: $('#uiLabelToday').val()},
                        {type: 'Text',  name: 'uiLabelYesterday',                   value: $('#uiLabelYesterday').val()},

                        {type: 'Text',  name: 'uiLabelSearchPlaceholder',           value: $('#uiLabelSearchPlaceholder').val()},
                        {type: 'Text',  name: 'uiLabelSearchErrorNoResults',        value: $('#uiLabelSearchErrorNoResults').val()},
                        {type: 'Text',  name: 'uiLabelSearchErrorNoResultsP2',      value: $('#uiLabelSearchErrorNoResultsP2').val()},
                        {type: 'Text',  name: 'uiLabelSearchErrorTimeout',          value: $('#uiLabelSearchErrorTimeout').val()},
                        {type: 'Text',  name: 'uiLabelSearchFacet1',                value: $('#uiLabelSearchFacet1').val()},
                        {type: 'Text',  name: 'uiLabelSearchFacet2',                value: $('#uiLabelSearchFacet2').val()},
                        {type: 'Text',  name: 'uiLabelSearchFacet3',                value: $('#uiLabelSearchFacet3').val()},
                        {type: 'Text',  name: 'uiLabelSearchFacet4',                value: $('#uiLabelSearchFacet4').val()},
                        {type: 'Text',  name: 'uiLabelSearchFacet5',                value: $('#uiLabelSearchFacet5').val()},

                        {type: 'Text',  name: 'shareEmailSubject',                  value: $('#shareEmailSubject').val()},

                        {type: 'Text',  name: 'ceoCategory1',                       value: $('#ceoCategory1').val()},
                        {type: 'Text',  name: 'ceoCategory2',                       value: $('#ceoCategory2').val()},
                        {type: 'Text',  name: 'ceoCategory3',                       value: $('#ceoCategory3').val()},
                        {type: 'Text',  name: 'ceoCategory4',                       value: $('#ceoCategory4').val()},
                        {type: 'Text',  name: 'ceoCategory5',                       value: $('#ceoCategory5').val()},
                        {type: 'Text',  name: 'ceoCategory6',                       value: $('#ceoCategory6').val()},
                        {type: 'Text',  name: 'ceoCategory1Image',                  value: $('#ceoCategory1Image').val()},
                        {type: 'Text',  name: 'ceoCategory2Image',                  value: $('#ceoCategory2Image').val()},
                        {type: 'Text',  name: 'ceoCategory3Image',                  value: $('#ceoCategory3Image').val()},
                        {type: 'Text',  name: 'ceoCategory4Image',                  value: $('#ceoCategory4Image').val()},
                        {type: 'Text',  name: 'ceoCategory5Image',                  value: $('#ceoCategory5Image').val()},
                        {type: 'Text',  name: 'ceoCategory6Image',                  value: $('#ceoCategory6Image').val()},
                     
                        {type: 'Text',  name: 'searchGSARequestTimeout',            value: $('#searchGSARequestTimeout').val()},
                    ];

                    return attributes;
                });

                if (!{!disableAll}) {
                    initializeTagPathSelectors();
                    initializeMultiTagPathSelectors();

                    $('#notificationsCheckInterval').acceptChars('digits');
                    $('#notificationsMaxLoad, #notificationsPerPage').acceptChars('digits length 3');
                } else {
                    initializeMultiTagPathSelectorsDisabled();
                }

                initCheckBoxes();
            });
        </script>

        <ul class="OCMSTabBar"><!-- no space between inline tab li's -->
            <li class="active" data-tabsheet="general">General</li><li data-tabsheet="uiText">UI Text</li><li data-tabsheet="advanced">Advanced</li>
        </ul>

        <div class="OCMSTabSheet_c">
