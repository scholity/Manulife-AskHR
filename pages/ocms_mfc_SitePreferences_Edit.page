                        {type: 'Text',  name: 'uiTitleNotifications',               value: $('#uiTitleNotifications').val()},
                        {type: 'Text',  name: 'uiTitleLaunchPad',                   value: $('#uiTitleLaunchPad').val()},
                        {type: 'Text',  name: 'uiTopIconsGroup',                    value: $('#uiTopIconsGroup').val()},
                        {type: 'Text',  name: 'uiTitleSearch',                      value: $('#uiTitleSearch').val()},

                        {type: 'Text', name: 'uiTitleFacebook',                     value: $('#uiTitleFacebook').val()},
