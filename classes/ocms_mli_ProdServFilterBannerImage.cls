    /**
     * @description  Generates mark-up for the banner image.
     * @return  String  A string of mark-up used to generate the banner image.
     */
    global override String getHTML() {

        String sProdServFiltBannerHtml = '';

        sProdServFiltBannerHtml += '<!-- >>> ocms_mli_ProdServFilterBannerImage.getHTML(...) -->'; // to fix any jQuery parsing issues, see: https://pl5.projectlocker.com/Stantive/PSTools/trac/wiki/gotcha/OCMSCachingMissingMarkup
        sProdServFiltBannerHtml += '<div class="bannerImage">';
        sProdServFiltBannerHtml +=         '<div class="mask"></div>';
        sProdServFiltBannerHtml +=         '<img src="' + ((this.sMobileImage != null) ? this.sMobileImage : '') + '" alt="' + ((this.sBannerImageAlt != null) ? this.sBannerImageAlt : '') + '" class="mobileOnly">';
        sProdServFiltBannerHtml +=         '<img src="' + ((this.sTabletImage != null) ? this.sTabletImage : '') + '" alt="' + ((this.sBannerImageAlt != null) ? this.sBannerImageAlt : '') + '" class="tabletOnly">';
        sProdServFiltBannerHtml +=         '<img src="' + ((this.sDesktopImage != null) ? this.sDesktopImage : '') + '" alt="' + ((this.sBannerImageAlt != null) ? this.sBannerImageAlt : '') + '" class="desktopOnly">';
        sProdServFiltBannerHtml += '</div>';
        sProdServFiltBannerHtml += '<!-- <<< ocms_mli_ProdServFilterBannerImage.getHTML(...) -->';
        return sProdServFiltBannerHtml;
    }
    
