public class POSArchives {
    private List<Archive> ArcList = new List<Archive>();
    
    public List<Archive> ArchiveData { get {return ArcList;} }
    public string LatestFile { get {return GetLatestFile(); } }

    public class Archive {
        public string Name { get; set; }
        public string ThumbnailImage { get; set; }
        public string ArticleImage { get; set; }
    }
    
    public POSArchives () {
        GenerataArchiveData();
    }
    
    private void GenerataArchiveData () {
        string s;
        Archive a;
        folder f = [select id from folder where Name = 'POS Archives'];
        List<document> tnl = [select id, Name from document where FolderId = :f.id and Name like 'tn%'];
        
        for (document img : [select id, Name
                             from document
                             where FolderId = :f.id and (not Name like 'tn%')
                             order by Name desc]
            ) {
                for (document tn : tnl) {
                    s = img.Name.substring(0, img.Name.indexOf('.'));
                    
                    if (tn.Name.substring(0, tn.Name.indexOf('.')) == ('tn' + s)) {
                        s = GetName(s);
                        
                        if (s != ' ') {
                            a = new Archive();
                            a.Name = s;
                            a.ThumbnailImage = tn.id;
                            a.ArticleImage = img.id;
                            ArcList.add(a);
                        }
                        
                        break;
                    }
                }
            }
    }
    
    private string GetName (string FileName) {
        integer month;
        string result = ' ';
        
        if (FileName.length() == 6 && FileName.isNumeric()) {
            month = integer.valueOf(FileName.substring(4, 6));
            
            if (month == 1) {
                result = 'January ' + FileName.substring(0, 4);
            } else if (month == 2) {
                result = 'February ' + FileName.substring(0, 4);
            } else if (month == 3) {
                result = 'March ' + FileName.substring(0, 4);
            } else if (month == 4) {
                result = 'April ' + FileName.substring(0, 4);
            } else if (month == 5) {
                result = 'May ' + FileName.substring(0, 4);
            } else if (month == 6) {
                result = 'June ' + FileName.substring(0, 4);
            } else if (month == 7) {
                result = 'July ' + FileName.substring(0, 4);
            } else if (month == 8) {
                result = 'August ' + FileName.substring(0, 4);
            } else if (month == 9) {
                result = 'September ' + FileName.substring(0, 4);
            } else if (month == 10) {
                result = 'October ' + FileName.substring(0, 4);
            } else if (month == 11) {
                result = 'November ' + FileName.substring(0, 4);
            } else if (month == 12) {
                result = 'December ' + FileName.substring(0, 4);
            }
        }
        
        return result;
    }
    
    public string GetLatestFile () {
        return ArcList[0].ArticleImage;
    }
    
